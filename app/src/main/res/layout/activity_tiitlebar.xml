<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lee="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.zhiyuan.androidwidget.widget.MyTitlebar
        android:id="@+id/mytitlebar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        lee:left_button_drawable="@drawable/back"
        lee:left_button_visible="true"
        lee:right_button_drawable="@drawable/add"
        lee:title_backgroud_color="@color/colorGreen"
        lee:title_text="Home"></com.zhiyuan.androidwidget.widget.MyTitlebar>

    <com.zhiyuan.androidwidget.widget.MyTitlebar
        android:id="@+id/mytitlebar1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        lee:left_button_drawable="@drawable/back"
        lee:left_button_visible="true"
        lee:right_button_drawable="@drawable/add"
        lee:title_backgroud_color="@color/colorYellow"
        lee:title_text="Home"></com.zhiyuan.androidwidget.widget.MyTitlebar>

    <com.zhiyuan.androidwidget.widget.TagsLayout
        android:id="@+id/taglayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        lee:tagHorizontalSpace="10dp"
        lee:tagVerticalSpace="10dp"></com.zhiyuan.androidwidget.widget.TagsLayout>


    <com.zhiyuan.androidwidget.widget.SquareView
        android:layout_width="match_parent"
        android:layout_height="500dp" />
</LinearLayout>
